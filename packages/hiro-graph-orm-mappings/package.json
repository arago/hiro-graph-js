        "@arago/OGIT": "https://github.com/arago/OGIT.git",
        "@arago/eslint-config-arago": "github:arago/eslint-config-arago",
        "@types/mustache": "^0.8.32",
        "@types/shelljs": "^0.8.2",
        "lerna": "^3.10.8",
        "mustache": "^3.0.1",
        "prettier": "^1.16.4",
        "shelljs": "^0.8.3",
        "ts-node": "^8.0.2",
        "tslib": "^1.9.3",
        "tslint": "^5.12.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.4.5"
    },
    "publishConfig": {

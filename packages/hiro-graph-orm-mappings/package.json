{
    "name": "@hiro-graph/orm-mappings",
    "version": "2.0.11",
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "description": "Automatic generation of ORM mapping based on OGIT",
    "license": "MIT",
    "scripts": {
        "build": "yarn lint && TS_NODE_FILES=true ts-node ./src/index && yarn format",

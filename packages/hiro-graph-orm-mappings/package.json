        "shelljs": "^0.8.3",
        "ts-node": "^7.0.1",
        "tslib": "^1.9.3",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "typescript": "^3.1.3"
    },
    "dependencies": {
        "@hiro-graph/orm": "^1.0.15"
    }

{
    "name": "@hiro-graph/client",
    "repository": "arago/hiro-graph-js",
    "version": "7.3.15",
    "description": "Javascript client for HIRO Graph REST API. Works in both node and browser.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "sideEffects": false,
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "rm -rf lib/* && yarn tsc",
        "prepublish": "yarn test && yarn build",
        "test": "madge --circular src/index.ts && jest --silent",
        "test:ci": "jest --silent --ci --reporters=default --reporters=jest-junit"
    },
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.9.2",
        "isomorphic-fetch": "^2.2.1",
        "rxjs": "^6.5.4",
        "uid": "^1.0.0",
        "websocket": "^1.0.25"
    },
    "devDependencies": {
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/jest": "^25.2.1",
        "@types/node-fetch": "2.5.4",
        "@types/websocket": "1.0.0",
        "jest": "^25.1.0",
        "madge": "^3.8.0",
        "ts-jest": "^25.3.1",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3"
    },
    "browserslist": [
        "chrome >= 64",
        "ie 11",
        "edge >= 15"
    ],
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "modules": false
                }
            ]
        ]
    },
    "jest-junit": {
        "suiteName": "@hiro-graph/client",
        "outputDirectory": "../../reports",
        "outputName": "hiro-graph-client.xml"
    },
    "publishConfig": {
        "access": "public"
    }
}

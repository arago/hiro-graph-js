{
    "name": "@hiro-graph/client",
    "repository": "arago/hiro-graph-js",
    "version": "7.4.0-rc.21",
    "description": "Javascript client for HIRO Graph REST API. Works in both node and browser.",
    "main": "lib/index.cjs.js",
    "module": "lib/index.esm.js",
    "types": "lib/index.d.ts",
    "sideEffects": false,
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "rollup -c",
        "prepublish": "yarn test && yarn build",
        "test": "madge --circular src/index.ts && jest --silent",
        "test:ci": "jest --silent --ci --reporters=default --reporters=jest-junit"
    },
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.14.0",
        "isomorphic-fetch": "^3.0.0",
        "isomorphic-ws": "^4.0.1",
        "lodash": "^4.17.21",
        "nanoid": "^3.1.23",
        "ws": "^7.4.5"
    },
    "devDependencies": {
        "@hiro-ui/eslint-config": "1.6.1",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-typescript": "^8.2.1",
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.168",
        "@types/node-fetch": "2.5.10",
        "@types/ws": "^7.4.4",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "lint-staged": "^11.0.0",
        "madge": "^4.0.2",
        "rollup": "^2.47.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "rxjs": "^7.0.0",
        "ts-jest": "^26.5.6",
        "ts-node": "^9.1.1",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    },
    "peerDependencies": {
        "rxjs": "~7.0.0"
    },
    "jest-junit": {
        "suiteName": "@hiro-graph/client",
        "outputDirectory": "../../reports",
        "outputName": "hiro-graph-client.xml"
    },
    "publishConfig": {
        "access": "public"
    }
}

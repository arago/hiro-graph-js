{
    "name": "@hiro-graph/client",
    "repository": "arago/hiro-graph-js",
    "version": "7.6.2",
    "description": "Javascript client for HIRO Graph REST API. Works in both node and browser.",
    "main": "lib/index.cjs.js",
    "module": "lib/index.esm.js",
    "types": "lib/index.d.ts",
    "sideEffects": false,
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "rollup -c",
        "prepublish": "yarn test && yarn build",
        "test": "madge --circular src/index.ts && jest --silent",
        "test:ci": "jest --silent --ci --reporters=default --reporters=jest-junit"
    },
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.15.4",
        "isomorphic-fetch": "^3.0.0",
        "isomorphic-ws": "^4.0.1",
        "lodash": "^4.17.21",
        "nanoid": "^3.1.28",
        "ws": "^8.2.2"
    },
    "devDependencies": {
        "@hiro-ui/eslint-config": "2.0.0",
        "@hiro-ui/prettier-config": "2.0.0",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.5",
        "@rollup/plugin-typescript": "^8.2.5",
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/jest": "^27.0.2",
        "@types/lodash": "^4.14.174",
        "@types/node-fetch": "3.0.3",
        "@types/ws": "^8.2.0",
        "husky": "^7.0.2",
        "jest": "^27.2.2",
        "jest-junit": "^12.3.0",
        "lint-staged": "^11.1.2",
        "madge": "^5.0.1",
        "rollup": "^2.57.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "rxjs": "^7.3.0",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    },
    "peerDependencies": {
        "rxjs": "~7.0.0"
    },
    "jest-junit": {
        "suiteName": "@hiro-graph/client",
        "outputDirectory": "../../reports",
        "outputName": "hiro-graph-client.xml"
    },
    "publishConfig": {
        "access": "public"
    }
}

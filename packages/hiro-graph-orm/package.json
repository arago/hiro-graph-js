{
    "name": "@hiro-graph/orm",
    "version": "6.1.4",
    "description": "OGIT ontology to sane application mappings",
    "repository": "arago/hiro-graph-js",
    "main": "lib/index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "rm -rf lib/* && BABEL_ENV=production babel src --out-dir lib",
        "prepublish": "yarn test && yarn build",
        "test": "jest --silent"
    },
    "license": "MIT",
    "dependencies": {
        "@hiro-graph/client": "^6.1.4",
        "@hiro-graph/gremlin": "^6.0.3",
        "@hiro-graph/lucene": "^6.0.7",
        "lodash.isplainobject": "^4.0.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.4",
        "@babel/core": "^7.15.5",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-transform-regenerator": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/plugin-transform-spread": "^7.14.6",
        "@babel/preset-env": "^7.15.6",
        "@types/websocket": "^1.0.4",
        "babel-jest": "^27.2.0",
        "jest": "^27.2.0",
        "eslint": "^5.3.0",
        "husky": "^1.1.2"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "modules": "commonjs"
                }
            ]
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-transform-regenerator",
            "@babel/plugin-transform-runtime"
        ]
    },
    "jest": {
        "testPathIgnorePatterns": [
            "<rootDir>[/\\\\](lib|node_modules)[/\\\\]"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
